{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "fcc_mobile_app",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-browser-flag=--disable-web-security"
      ]
    },
    {
      "name": "fcc_mobile_app (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "fcc_mobile_app (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "Flutter (Chrome)",
      "program": "lib/main.dart",
      "deviceId": "chrome",
      "request": "launch",
      "type": "dart"
    }
  ]
}